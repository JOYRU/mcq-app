{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/questions/(.*)",
        "dest": "/questions/$1"
      },
      {
        "src": "/exams/(.*)",
        "dest": "/exams/$1"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/auth/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  